{
  "name": "buyme/laravel-google-translate",
  "description": "Laravel package for the Google Translate API",
  "keywords": [
    "laravel",
    "package",
    "google",
    "translate",
    "google translate",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Developer",
      "email": "info@buymeua.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Byume\\GoogleTranslate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Byume\\GoogleTranslate\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "google/cloud-translate": "^1.2",
    "illuminate/support": "9.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.16",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "7.*",
    "phpunit/phpunit": "^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Byume\\GoogleTranslate\\GoogleTranslateServiceProvider"
      ],
      "aliases": {
        "GoogleTranslate": "Byume\\GoogleTranslate\\Facades\\GoogleTranslate"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
